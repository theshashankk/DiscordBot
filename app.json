{
  "name": "Tharki BoT",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/theshashankk/DiscordBot",
  "stack": "container",
  "env": {
    "PREFIX": {
      "description": "Cmd Handler",
      "required": true
    },
    "TOKEN": {
      "description": "A bot token discord.com/developer",
      "required": true
    },
    "CLIENT_ID": {
      "description": "Bot client ID. Can be found in your bots application page",
      "required": true
    },
    "REDIS_URL": {
      "description": "Redis URL. Can be left blank if Redis is opened locally",
      "required": true
    },
    "MESSAGE_TTL": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
      "value": "1"
    },
    "YOUTUBE_KEY": {
      "description": "Youtube API key, used for searching videos with `!youtube.",
      "required": true,
    },
   "IMGFLIP_USERNAME":{
     "discription": "Imgflip username and password, used for creating memes with `!meme`."
  "required": true
   },
  "IMGFLIP_PASSWORD":{
    "discription": "Imgflip username and password, used for creating memes with `!meme`."
    "required": true
},
  "CHAMPIONGG_KEY"
  "discription": "Champion.gg API key, used for the League of Legends `!lol` commands."
"required": false
},
 "RIOT_KEY"
  "discription": "Riot's API key, used for player and match search commands.
"
"required": false
},
"WOLFRAM_KEY"
"discription": " Wolfram Alpha API key, used to query Wolfram Alpha with `!wolfram`.
  . http://products.wolframalpha.com/developers "
"required": false
},
"PASTEBIN_KEY"
"discription": "Get a key here. http://pastebin.com/api "
"required": false
},
"POPKEY_KEY"
"discription": "Popkey API key, used for searching GIFs with `!popkey`.
 . http://pastebin.com/api "
"required": false
},
"RANDOM_KEY"
"discription": "Random.org API key, used for getting random numbers and strings with `!random`
 . https://api.random.org/api-keys "
"required" false
},
"CLEVERBOT_KEY"
"description": "Cleverbot API key, used for talking with `!chat`.
   https://www.cleverbot.com/api "
"required": false
},
"CARBON_KEY"
"description": "Carbon and Discord Bots key, used for displaying server count on specific websites.
  . https://www.carbonitex.net/discord/bots & https://bots.discord.pw/api "
"required": false
},
"DBOTS_KEY"
"description": "Carbon and Discord Bots key, used for displaying server count on specific websites.
  More information here. https://www.carbonitex.net/discord/bots & https://bots.discord.pw/api "
"required": false
},
"FEEDBACK_CHANNEL_ID"
"description": "Feedback Channel ID, used for storing feedback with `!feedback`."
   }
  },
  "addons": [{
   "plan": "heroku-postgresql",
   "options": {
    "version": "12"
   }
  }]
 }
